<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="adm_aisj_interested_options_view_tree" model="ir.ui.view">
        <field name="name">adm.aisj.interested.options.view.tree</field>
        <field name="model">adm_aisj.interested.options</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="adm_aisj_interested_options_action" model="ir.actions.act_window">
        <field name="name">AISJ interested options</field>
        <field name="res_model">adm_aisj.interested.options</field>
        <field name="view_mode">tree,search</field>
    </record>

    <menuitem id="adm_aisj_interested_options_menu"
              parent="adm.adm_custom_menu"
              action="adm_aisj.adm_aisj_interested_options_action"/>

</odoo>